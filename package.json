    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "^0.28.4",
    "del-cli": "^1.1.0",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
