    "build:production": "npm run build -- --env.environment production",
    "clean": "del-cli \"dist/**/*\"",
    "copy": "cp src/manifest.json dist/manifest.json;",
    "format": "prettier --write --single-quote \"src/**/*.js\"",
    "markdown": "md-magic",
    "start": "webpack-dev-server --env.environment development --hot --host 0.0.0.0 --port 8000",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
