    "lz-string": "^1.4.4",
    "normalize.css": "^7.0.0",
    "offline-plugin": "^4.8.3",
    "polished": "^1.4.0",
    "query-string": "^5.0.0",
    "radium": "^0.19.4",
    "react": "next",
    "react-dom": "next",
    "react-icons": "^2.2.5",
    "react-jss": "^7.1.0",
    "styled-components": "^2.1.2",
    "styled-jss": "^1.2.0",
    "styletron-react": "^2.5.8",
    "webfontloader": "^1.6.28"
