    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^16.0.0-rc.2",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.4",
    "webpack-config-assign": "^1.0.0",
    "webpack-dev-server": "^2.7.1",
    "worker-loader": "^0.8.1"
